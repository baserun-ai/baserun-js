{
  "name": "baserun",
  "version": "1.3.9",
  "description": "",
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "files": [
    "dist",
    "jest-preset.js"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "prepare": "tshy",
    "test:baserun": "jest --testPathPattern=\"/baserun/\" --config=jest.config.baserun.js",
    "test:normal": "jest --testPathIgnorePatterns=\"/baserun/\"",
    "build:grpc": "bash scripts/build_grpc.sh",
    "test": "concurrently 'npm run test:baserun' 'npm run test:normal'",
    "vitest": "vitest",
    "lint": "eslint --fix --ext .js,.ts .",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "watch": "tsc --watch",
    "postinstall": "patch-package"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.6.2",
    "@types/google-protobuf": "^3.15.10",
    "@types/jest": "^29.5.8",
    "@types/json-stringify-safe": "^5.0.1",
    "@types/lodash.pick": "^4.4.9",
    "@types/node": "^20.9.0",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lodash.pick": "^4.4.0",
    "mocha": "^10.2.0",
    "openai": "^4.17.5",
    "patch-package": "^8.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.1",
    "tshy": "^1.8.0",
    "typescript": "^5.1.3",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.9",
    "google-protobuf": "^3.21.2",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "isomorphic-unfetch": "^4.0.2",
    "json-stringify-safe": "^5.0.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "openai": "^3.2.1 || ^4.0.0"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  }
}
