{
  "name": "baserun",
  "version": "2.0.0-alpha.48",
  "description": "The Baserun TypeScript SDK",
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "files": [
    "dist",
    "jest-preset.js"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "prepare": "tshy",
    "test:baserun": "jest --testPathPattern=\"/baserun/\" --config=jest.config.baserun.js",
    "test:normal": "jest --testPathIgnorePatterns=\"/baserun/\"",
    "build:grpc": "protoc --ts_opt client_grpc1 --ts_out ./src/v1/gen --proto_path src/v1/protos src/v1/protos/baserun.proto",
    "test": "concurrently 'npm run test:baserun' 'npm run test:normal'",
    "vitest": "vitest",
    "lint": "eslint --fix --ext .js,.ts .",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "watch": "tsc --watch",
    "publish:alpha": "rimraf dist && rimraf .tshy && npm run prepare && yarn publish --tag=alpha"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.9.1",
    "@bufbuild/buf": "^1.28.1",
    "@bufbuild/protobuf": "^1.4.2",
    "@bufbuild/protoc-gen-es": "^1.4.2",
    "@connectrpc/connect": "^1.1.3",
    "@connectrpc/connect-node": "^1.1.3",
    "@connectrpc/protoc-gen-connect-es": "^1.1.3",
    "@types/google-protobuf": "^3.15.10",
    "@types/jest": "^29.5.8",
    "@types/json-stringify-safe": "^5.0.1",
    "@types/lodash.pick": "^4.4.9",
    "@types/node": "^20.9.0",
    "@types/resolve": "^1.20.6",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lodash.pick": "^4.4.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tshy": "^1.8.1",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.9",
    "@protobuf-ts/plugin": "^2.9.1",
    "@types/debug": "^4.1.12",
    "debug": "^4.3.4",
    "fast-glob": "^3.3.2",
    "globby": "^14.0.0",
    "google-protobuf": "^3.21.2",
    "ignore": "5.2.4",
    "import-meta-resolve": "^4.0.0",
    "isomorphic-unfetch": "^4.0.2",
    "json-stringify-safe": "^5.0.1",
    "openai": "^4.19.1",
    "pkg-dir": "^8.0.0",
    "resolve": "^1.22.8",
    "resolve-pkg": "^2.0.0",
    "slash": "^5.1.0",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "openai": "^4.19.0"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  }
}
